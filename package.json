{
	"engines": {
		"node": "20.x"
	},
	"name": "backomydog",
	"version": "1.0.0",
	"description": "backend du projet o'mydog",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development node --watch index.js",
		"start": "NODE_ENV=production node index.js",
		"createdb": "psql -U <dbuser> -d <dbname> -f ./migrations/deploy/init-migration.sql",
		"seeding": "psql -U <dbuser> -d <dbname> -f ./data/seeding.sql",
		"createdb:seeding": "npm run createdb && npm run seeding"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/O-clock-Onigiri/projet-6-o-mydog-back.git"
	},
