	},
	"homepage": "https://github.com/O-clock-Onigiri/projet-6-o-mydog-back#readme",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cloudinary": "^2.2.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-jsdoc-swagger": "^1.8.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"multer": "^1.4.5-lts.1",
